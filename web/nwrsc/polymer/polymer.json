{
  "entrypoint": "index.html",
  "shell": "src/components/announcer-panel.js",
  "sources": [
   "src/**",
   "package.json"
  ],
  "extraDependencies": [
    "node_modules/@webcomponents/webcomponentsjs/**"
  ],
  "builds": [
    {
      "name": "devel",
      "browserCapabilities": [ "es2015" ],
      "js": {
        "minify": false,
        "compile": "es2015"
      },
      "css": {
        "minify": false
      },
      "html": {
        "minify": false
      },
      "bundle": false
    },
    {
      "name": "bundle",
      "browserCapabilities": [ "es2015" ],
      "js": {
        "minify": true,
        "compile": "es2015"
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true
    }
  ],

  "moduleResolution": "node",
  "npm": true
}
